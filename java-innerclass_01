package com.innerclass;

public class innerclass_01 {
    public static void main(String[] args) {

    }
}
// 外部类
class Outer{
    // 属性
    private int n1 = 100;
    // 方法
    public void hi(){
        System.out.println("hi()~~~");
    }
    // 构造器
    public Outer(int n1) {
        this.n1 = n1;
    }
    // 代码块
    {
        System.out.println("代码块...");
    }
    // 内部类
    class inter{

    }
}
